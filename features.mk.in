INCLUDE_CHARSTRING = @INCLUDE_CHARSTRING@
INCLUDE_CHARACTER = @INCLUDE_CHARACTER@
INCLUDE_INTERVALTIMER = @INCLUDE_INTERVALTIMER@
INCLUDE_MATH = @INCLUDE_MATH@
INCLUDE_RAWBUFFER = @INCLUDE_RAWBUFFER@
INCLUDE_MEMORYMAP = @INCLUDE_MEMORYMAP@
ifeq (@HAS_THREADS@,yes)
	INCLUDE_MUTEX = @INCLUDE_MUTEX@
endif
INCLUDE_SERIALPORT = @INCLUDE_SERIALPORT@
ifeq ($(INCLUDE_SERIALPORT),1)
	INCLUDE_SERIALPORTPROFILE = @INCLUDE_SERIALPORTPROFILE@
endif

INCLUDE_PARAMETERSTRING = @INCLUDE_PARAMETERSTRING@
ifeq ($(INCLUDE_CHARSTRING),1)
	INCLUDE_ENVIRONMENT = @INCLUDE_ENVIRONMENT@
	INCLUDE_COMMANDLINE = @INCLUDE_COMMANDLINE@
	INCLUDE_DATETIME = @INCLUDE_DATETIME@
	INCLUDE_TIMEZONEFILE = @INCLUDE_TIMEZONEFILE@
	INCLUDE_GROUPENTRY = @INCLUDE_GROUPENTRY@
	INCLUDE_PASSWDENTRY = @INCLUDE_PASSWDENTRY@
	INCLUDE_SERVICEENTRY = @INCLUDE_SERVICEENTRY@
	INCLUDE_SHADOWENTRY = @INCLUDE_SHADOWENTRY@
	INCLUDE_RPCENTRY = @INCLUDE_RPCENTRY@
	INCLUDE_PROTOCOLENTRY = @INCLUDE_PROTOCOLENTRY@
	INCLUDE_HOSTENTRY = @INCLUDE_HOSTENTRY@
	INCLUDE_DYNAMICLIB = @INCLUDE_DYNAMICLIB@
	INCLUDE_CRYPT = @INCLUDE_CRYPT@
endif

INCLUDE_VARIABLEBUFFER = @INCLUDE_VARIABLEBUFFER@
ifeq ($(INCLUDE_VARIABLEBUFFER),1)
	ifeq ($(INCLUDE_CHARSTRING),1)
		INCLUDE_STRINGBUFFER = @INCLUDE_STRINGBUFFER@
		ifeq ($(INCLUDE_STRINGBUFFER),1)
			INCLUDE_ERRORHANDLER = @INCLUDE_ERRORHANDLER@
			ifeq ($(INCLUDE_ERRORHANDLER),1)
				INCLUDE_XMLSAX = @INCLUDE_XMLSAX@
				ifeq ($(INCLUDE_XMLSAX),1)
					INCLUDE_XMLDOM = @INCLUDE_XMLDOM@
					INCLUDE_DTD = @INCLUDE_DTD@
				endif
			endif
		endif
	endif
endif

INCLUDE_FILESYSTEM = @INCLUDE_FILESYSTEM@
ifeq ($(INCLUDE_CHARSTRING),1)
	INCLUDE_DIRECTORY = @INCLUDE_DIRECTORY@
	INCLUDE_FILEDESCRIPTOR = @INCLUDE_FILEDESCRIPTOR@
	ifeq ($(INCLUDE_FILEDESCRIPTOR),1)
		INCLUDE_FILE = @INCLUDE_FILE@
		ifeq ($(INCLUDE_FILE),1)
			INCLUDE_DEVICE = @INCLUDE_DEVICE@
		endif
		INCLUDE_DATATRANSPORT = @INCLUDE_DATATRANSPORT@
		ifeq ($(INCLUDE_DATATRANSPORT),1)
			INCLUDE_CLIENT = @INCLUDE_CLIENT@
			ifeq ($(INCLUDE_CLIENT),1)
				ifeq ($(INCLUDE_PROTOCOLENTRY),1)
					ifeq ($(INCLUDE_HOSTENTRY),1)
						INCLUDE_INETCLIENT = @INCLUDE_INETCLIENT@
					endif
				endif
				INCLUDE_UNIXCLIENT = @INCLUDE_UNIXCLIENT@
			endif
			INCLUDE_SERVER = @INCLUDE_SERVER@
			ifeq ($(INCLUDE_SERVER),1)
				INCLUDE_INETSERVER = @INCLUDE_INETSERVER@
				INCLUDE_UNIXSERVER = @INCLUDE_UNIXSERVER@
			endif
		endif
	endif
endif

INCLUDE_LISTENER = @INCLUDE_LISTENER@
ifeq ($(INCLUDE_CHARSTRING),1)
	INCLUDE_LOGGER = @INCLUDE_LOGGER@
endif
INCLUDE_MEMORYPOOL = @INCLUDE_MEMORYPOOL@

INCLUDE_SIGNALCLASSES = @INCLUDE_SIGNALCLASSES@

ifeq ($(INCLUDE_PASSWDENTRY),1)
	ifeq ($(INCLUDE_GROUPENTRY),1)
		ifeq ($(INCLUDE_SIGNALCLASSES),1)
			ifeq ($(INCLUDE_CHARSTRING),1)
				INCLUDE_DAEMONPROCESS = @INCLUDE_DAEMONPROCESS@
			endif
		endif
		INCLUDE_SEMAPHORESET = @INCLUDE_SEMAPHORESET@
		INCLUDE_SHAREDMEMORY = @INCLUDE_SHAREDMEMORY@
	endif
endif

ifeq ($(INCLUDE_CHARSTRING),1)
	INCLUDE_PERMISSIONS = @INCLUDE_PERMISSIONS@
	INCLUDE_REGULAREXPRESSION = @INCLUDE_REGULAREXPRESSION@
endif
INCLUDE_RANDOMNUMBER = @INCLUDE_RANDOMNUMBER@

#INCLUDE_DICTIONARY = @INCLUDE_DICTIONARY@
#INCLUDE_LIST = @INCLUDE_LIST@
